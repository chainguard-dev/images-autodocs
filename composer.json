{
  "name": "chainguard-dev/images-autodocs",
  "description": "Chainguard Images Automated Documentation",
  "license": "MIT",
  "homepage": "https://github.com/chainguard-dev/images-autodocs",
  "keywords": ["docs","chainguard", "containers"],
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "repositories": [
    {
      "type": "path",
      "url": "../autodocs"
    }
  ],
  "require": {
    "php": ">=8.1",
    "minicli/minicli": "^4.2",
    "minicli/command-help": "^1.0",
    "erikaheidi/autodocs": "dev-main",
    "minicli/curly": "^0.2.2"
  },
  "require-dev": {
    "pestphp/pest": "^1.23",
    "laravel/pint": "^1.10"
  },
  "scripts": {
    "lint" : ["pint"],
    "test:lint" : ["pint --test"],
    "test:unit" : ["pest"],
    "test" : [
      "@test:lint",
      "@test:unit"
    ]
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
